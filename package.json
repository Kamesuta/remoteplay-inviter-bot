{
  "name": "nodejs-typescript-eslint-template",
  "version": "1.0.0",
  "description": "Robust TypeScript Template with eslint for Node.js",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --import tsx/esm src/index.ts",
    "register": "node --import tsx/esm src/commands.ts",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "lint": "tsc --noEmit && eslint --cache --fix",
    "prettier": "prettier --write",
    "lint-staged": "lint-staged",
    "prepare": "husky"
  },
  "author": "Kamesuta",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/express": "^4.17.21",
    "@types/i18n": "^0.13.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.13.0",
    "@types/node-schedule": "^2.1.7",
    "@types/ws": "^8.5.11",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^48.2.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prisma": "^5.16.2",
    "rimraf": "^5.0.7",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.11.0",
    "typescript-eslint": "^8.0.0-alpha.13"
  },
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "i18n": "^0.15.1",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "node-schedule": "^2.1.1",
    "semver": "^7.6.3",
    "toml": "^3.0.0",
    "ws": "^8.18.0",
    "yaml": "^2.5.0"
  },
  "overrides": {
    "@discordjs/builders": "^1.9.0-dev.1721153954-efa16a609",
    "discord-api-types": "0.37.90"
  }
}
