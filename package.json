{
  "name": "remoteplay-inviter-bot",
  "version": "1.0.0",
  "description": "Invite your friends via Discord and play Steam games together for free!",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --import tsx/esm src/index.ts",
    "register": "node --import tsx/esm src/commands.ts",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "lint": "tsc --noEmit && eslint --cache --fix",
    "prettier": "prettier --write",
    "lint-staged": "lint-staged",
    "prepare": "npx if-env NODE_ENV=production && echo 'Skipping husky in production' || husky",
    "heroku-postbuild": "prisma generate && prisma migrate deploy"
  },
  "author": "Kamesuta",
  "license": "MIT",
  "engines": {
    "node": "22.x"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/express": "^5.0.1",
    "@types/i18n": "^0.13.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.14.1",
    "@types/node-schedule": "^2.1.7",
    "@types/ws": "^8.18.1",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsdoc": "^50.6.10",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^8.31.0"
  },
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "if-env": "^1.0.4",
    "express": "^5.1.0",
    "i18n": "^0.15.1",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "node-schedule": "^2.1.1",
    "prisma": "^6.6.0",
    "semver": "^7.7.1",
    "toml": "^3.0.0",
    "tsx": "^4.19.3",
    "ws": "^8.18.1",
    "yaml": "^2.7.1"
  },
  "overrides": {
    "@discordjs/builders": "^1.9.0-dev.1721153954-efa16a609",
    "discord-api-types": "0.37.90"
  }
}
